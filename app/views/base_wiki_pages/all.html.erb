<%= render 'wiki_header'%>

<div class="container">
    <div class="thumbnail">
<%= wiki_page_style -%>

<h1><%=wt 'All pages' %></h1>


<ul>
  <% wiki_paginate @pages do -%>
    <% @pages.each do |page| -%>
      <li><%= link_to page.title, wiki_page_path( page ) %></li>
    <% end -%>
  <% end -%>
</ul>
</div>
</div>
